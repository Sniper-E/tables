{
	"name": "sniper/tables",
	"type": "phpbb-extension",
	"description": "Inserts a table up to 7 columns and 10 rows in your post.",
	"homepage": "https://www.sniper-e.com",
	"version": "2.1.4",
	"time": "2020-01-12",
	"keywords": ["phpbb", "extension", "tables"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Sniper_E",
			"homepage": "https://sniper-e.com",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=7.1.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Tables",
		"soft-require": {
			"phpbb/phpbb": "3.3.0, >="
		},
		"version-check": {
			"host": "www.sniper-e.com",
			"directory": "/versions",
			"filename": "tables_version.json",
			"ssl": true
		}
	}
}
